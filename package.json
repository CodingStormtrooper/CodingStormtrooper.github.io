{
  "name": "codingstormtrooper.github.io",
  "version": "1.0.0",
  "description": "Static multi-page site suitable for GitHub Pages.",
  "main": "index.js",
  "scripts": {
    "build:css": "npx @tailwindcss/cli -i src/assets/styles.css -o _site/assets/styles.css --minify",
    "build:html": "npx @11ty/eleventy",
    "build": "npm run build:html && npm run build:css",
    "start": "npx @11ty/eleventy --serve"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CodingStormtrooper/CodingStormtrooper.github.io.git"
  },
  "keywords": [],
