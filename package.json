  "license": "ISC",
  "bugs": {
    "url": "https://github.com/CodingStormtrooper/CodingStormtrooper.github.io/issues"
  },
  "homepage": "https://github.com/CodingStormtrooper/CodingStormtrooper.github.io#readme",
  "devDependencies": {
    "@11ty/eleventy": "^3.1.2",
    "@tailwindcss/cli": "^4.1.18",
    "tailwindcss": "^4.1.18"
  }
}
